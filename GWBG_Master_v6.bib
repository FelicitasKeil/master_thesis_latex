@article{van_holten_gravitational_2019,
	title = {Gravitational waves from generalized newtonian sources},
	volume = {67},
	issn = {00158208},
	url = {http://arxiv.org/abs/1809.05670},
	doi = {10.1002/prop.201800083},
	abstract = {I review the elementary theory of gravitational waves on a Minkowski background and the quadrupole approximation. The modified conservation laws for energy and momentum keeping track of the gravitational-wave flux are presented. The theory is applied to two-body systems in bound and scattering states subject to newtonian gravity generalized to include a 1/r3 force allowing for orbital precession. The evolution of the orbits is studied in the adiabatic approximation. From these results I derive the conditions for capture of two bodies to form a bound state by the emission of gravitational radiation.},
	pages = {1800083},
	number = {3},
	journaltitle = {Fortschritte der Physik},
