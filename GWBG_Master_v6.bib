
@article{di_dio_classgal_2013,
	title = {The {CLASSgal} code for Relativistic Cosmological Large Scale Structure},
	volume = {2013},
	issn = {1475-7516},
	url = {http://arxiv.org/abs/1307.1459},
	doi = {10.1088/1475-7516/2013/11/044},
	abstract = {We present some accurate and efficient computations of large scale structure observables, obtained with a modified version of the {CLASS} code which is made publicly available. This code includes all relativistic corrections and computes both the power spectrum Cl(z1,z2) and the corresponding correlation function xi(theta,z1,z2) in linear perturbation theory. For Gaussian initial perturbations, these quantities contain the full information encoded in the large scale matter distribution at the level of linear perturbation theory. We illustrate the usefulness of our code for cosmological parameter estimation through a few simple examples.},
	pages = {044--044},
	number = {11},
	journaltitle = {Journal of Cosmology and Astroparticle Physics},
	shortjournal = {J. Cosmol. Astropart. Phys.},
	author = {Di Dio, Enea and Montanari, Francesco and Lesgourgues, Julien and Durrer, Ruth},
	urldate = {2023-08-09},
	date = {2013-11-20},
	eprinttype = {arxiv},
	eprint = {1307.1459 [astro-ph]},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics},
	file = {arXiv Fulltext PDF:/home/felic/snap/zotero-snap/common/Zotero/storage/SZRE8BEB/Di Dio et al. - 2013 - The CLASSgal code for Relativistic Cosmological La.pdf:application/pdf;arXiv.org Snapshot:/home/felic/snap/zotero-snap/common/Zotero/storage/3CE57JII/1307.html:text/html},
}

