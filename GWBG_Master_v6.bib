	url = {http://arxiv.org/abs/2110.01634},
	doi = {10.3847/1538-4357/ac64a3},
	shorttitle = {The redshift evolution of the binary black hole merger rate},
	abstract = {Gravitational wave detectors are starting to reveal the redshift evolution of the binary black hole ({BBH}) merger rate, \$R\_\{{\textbackslash}mathrm\{{BBH}\}\}(z)\$. We make predictions for \$R\_\{{\textbackslash}mathrm\{{BBH}\}\}(z)\$ as a function of black hole mass for systems originating from isolated binaries. To this end, we investigate correlations between the delay time and black hole mass by means of the suite of binary population synthesis simulations, {COMPAS}. We distinguish two channels: the common envelope ({CE}), and the stable Roche-lobe overflow ({RLOF}) channel, characterised by whether the system has experienced a common envelope or not. We find that the {CE} channel preferentially produces {BHs} with masses below about \$30{\textbackslash}rm\{M\}\_\{{\textbackslash}odot\}\$ and short delay times (\$t\_\{{\textbackslash}rm delay\} {\textbackslash}lesssim 1\$Gyr), while the stable {RLOF} channel primarily forms systems with {BH} masses above \$30{\textbackslash}rm\{M\}\_\{{\textbackslash}odot\}\$ and long delay times (\$t\_\{{\textbackslash}rm delay\} {\textbackslash}gtrsim 1\$Gyr). We provide a new fit for the metallicity specific star-formation rate density based on the Illustris {TNG} simulations, and use this to convert the delay time distributions into a prediction of \$R\_\{{\textbackslash}mathrm\{{BBH}\}\}(z)\$. This leads to a distinct redshift evolution of \$R\_\{{\textbackslash}mathrm\{{BBH}\}\}(z)\$ for high and low primary {BH} masses. We furthermore find that, at high redshift, \$R\_\{{\textbackslash}mathrm\{{BBH}\}\}(z)\$ is dominated by the {CE} channel, while at low redshift it contains a large contribution (\${\textbackslash}sim 40{\textbackslash}\%\$) from the stable {RLOF} channel. Our results predict that, for increasing redshifts, {BBHs} with component masses above \$30{\textbackslash}rm\{M\}\_\{{\textbackslash}odot\}\$ will become increasingly scarce relative to less massive {BBH} systems. Evidence of this distinct evolution of \$R\_\{{\textbackslash}mathrm\{{BBH}\}\}(z)\$ for different {BH} masses can be tested with future detectors.},
	pages = {17},
	number = {1},
	journaltitle = {The Astrophysical Journal},
	shortjournal = {{ApJ}},
	author = {van Son, L. A. C. and de Mink, S. E. and Callister, T. and Justham, S. and Renzo, M. and Wagg, T. and Broekgaarden, F. S. and Kummer, F. and Pakmor, R. and Mandel, I.},
	urldate = {2023-08-10},
	date = {2022-05-01},
	eprinttype = {arxiv},
	eprint = {2110.01634 [astro-ph, physics:gr-qc]},
	keywords = {General Relativity and Quantum Cosmology, Astrophysics - Astrophysics of Galaxies, Astrophysics - High Energy Astrophysical Phenomena},
	file = {arXiv Fulltext PDF:/home/felic/snap/zotero-snap/common/Zotero/storage/EY7NH87F/van Son et al. - 2022 - The redshift evolution of the binary black hole me.pdf:application/pdf;arXiv.org Snapshot:/home/felic/snap/zotero-snap/common/Zotero/storage/7RVYMH5D/2110.html:text/html},
