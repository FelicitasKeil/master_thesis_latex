	urldate = {2023-08-10},
	date = {2020-05-11},
	eprinttype = {arxiv},
	eprint = {1909.11627 [astro-ph, physics:gr-qc]},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics, General Relativity and Quantum Cosmology},
	file = {arXiv Fulltext PDF:/home/felic/snap/zotero-snap/common/Zotero/storage/XXNSH93K/Bertacca et al. - 2020 - Projection effects on the observed angular spectru.pdf:application/pdf;arXiv.org Snapshot:/home/felic/snap/zotero-snap/common/Zotero/storage/LHYT2WDZ/1909.html:text/html},
