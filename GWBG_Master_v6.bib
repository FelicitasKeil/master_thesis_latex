	keywords = {General Relativity and Quantum Cosmology, Astrophysics - High Energy Astrophysical Phenomena, Astrophysics - Instrumentation and Methods for Astrophysics, High Energy Physics - Experiment, Nuclear Experiment},
	file = {arXiv Fulltext PDF:/home/felic/snap/zotero-snap/common/Zotero/storage/9MZHWIZR/Riles - 2013 - Gravitational Waves Sources, Detectors and Search.pdf:application/pdf;arXiv.org Snapshot:/home/felic/snap/zotero-snap/common/Zotero/storage/ZBLV7YP9/1209.html:text/html},
